<?xml version="1.0" encoding="utf-8"?>
<com.color.support.widget.ColorMaxHeightDraggableVerticalLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/color_draggable_vertical_linear_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <color.support.p043v7.widget.Toolbar
        android:id="@+id/bottom_sheet_toolbar"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginBottom="10dp"/>
    <FrameLayout
        android:id="@+id/title_view_container"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <FrameLayout
        android:id="@+id/panel_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</com.color.support.widget.ColorMaxHeightDraggableVerticalLinearLayout>
